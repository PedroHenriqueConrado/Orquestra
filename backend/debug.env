# Configurações de Ambiente para Debugging
NODE_ENV=development
PORT=3001

# Banco de dados
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/orquestra_debug?schema=public"

# JWT
JWT_SECRET="chave_secreta_para_debugging_local"
JWT_EXPIRES_IN="30d"

# Logs e Debug
LOG_LEVEL=debug
DEBUG_PRISMA=true

# CORS
CORS_ORIGIN="http://localhost:3000"

# Uploads
UPLOAD_DIR="uploads"
MAX_FILE_SIZE=10485760 # 10MB

# Timeouts mais longos para debugging
REQUEST_TIMEOUT=60000 # 60 segundos 